{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "globals": {
    "ts-jest": {
      "useESM": true
    }
  },
  "testEnvironmentOptions": {
    "loader": "ts-node/esm"
  }
}
